{
    "name": "carropublic/myinfo",
    "description": "PHP Integrating with MyInfo",
    "license": "MIT",
    "authors": [
        {
            "name": "Set Kyar Wa Lar <Universe>",
            "email": "universe@carro.co",
            "homepage": "https://carro.sg"
        }
    ],
    "homepage": "https://github.com/carropublic/myinfo",
    "keywords": ["Laravel", "MyInfo"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "spomky-labs/jose": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "CarroPublic\\MyInfo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarroPublic\\MyInfo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarroPublic\\MyInfo\\MyInfoServiceProvider"
            ],
            "aliases": {
                "MyInfo": "CarroPublic\\MyInfo\\Facades\\MyInfo"
            }
        }
    }
}
